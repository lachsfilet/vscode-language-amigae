{
    "name": "amiga-e",
    "displayName": "Amiga E",
    "description": "Language support for Amiga E",
    "publisher":"renkman",
    "author": {
        "name": "Jan Renken"
    },
    "version": "0.2.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/renkman/vscode-language-amigae.git"
      },
      "license": "GPL-3.0",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/icon.png",
    "contributes": {
        "languages": [{
            "id": "amigae",
            "aliases": ["Amiga E", "amigae"],
            "extensions": [".e"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "amigae",
            "scopeName": "source.e",
            "path": "./syntaxes/amigae.tmLanguage.json"
        }],
        "snippets": [{
            "language": "amigae",
            "path": "./snippets/amigae.json"
        }]
    }
}
